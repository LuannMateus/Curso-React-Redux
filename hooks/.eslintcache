n        </div>\n    )\n}\n\nexport default UseMemo;","/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/views/examples/useCallback.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/views/examples/useCallbackButtons.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/views/examples/useContext.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/data/DataContext.js",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/data/Store.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/views/examples/useReducer.jsx",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":15,"column":12,"nodeType":"85","messageId":"86","endLine":15,"endColumn":14},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'n3' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]
State(0);\n\n    useEffect(_ => {\n        setResult(sum(n1, n2))\n    }, [n1 ,n2])*/\n\n\n    return (\n        <div>\n            <Header\n                title='Hook UseMemo' subTitle='Retorna um valor memorizado!' />\n\n            <SectionExer className='center' title='Exercise - #01'>\n                <div className=\"center\">\n                    <span className=\"display\">{result}</span>\n                    <input type=\"number\" className=\"input\" \n                        onChange={e => setN1(parseInt(e.target.value))}/>\n                    <input type=\"number\" className=\"input\" \n                        onChange={e => setN2(parseInt(e.target.value))}/>\n                    <input type=\"number\" className=\"input\" \n                        onChange={e => setN3(parseInt(e.target.value))}/>\n                </div>\n            </SectionExer>\n        </div>\n    )\n}\n\nexport default UseMemo;","/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/views/examples/useCallback.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/views/examples/useCallbackButtons.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/views/examples/useContext.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/data/DataContext.js",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/data/Store.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/views/examples/useReducer.jsx",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/store/reducers/index.js",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/store/index.js",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/store/actions/number.js",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/store/actions/index.js",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux/hooks/src/store/actions/user.js",[],"/home/luan/Documents/Dev_Exercises/Curso-React-Redux